module i11eperf {
  @topic
  struct ou {
    unsigned long s;
  };

  @topic
  struct a32 {
    unsigned long s;
    octet xary[32 - 4];
  };

  @topic
  struct a128 {
    unsigned long s;
    octet xary[128 - 4];
  };

  @topic
  struct a1024 {
    unsigned long s;
    octet xary[1024 - 4];
  };

  @topic
  struct a48k {
    unsigned long s;
    octet xary[48*1024 - 4];
  };

  @topic
  struct a64k {
    unsigned long s;
    octet xary[64*1024 - 4];
  };

  @topic
  struct a1M {
    unsigned long s;
    octet xary[1024*1024 - 4];
  };

  @topic
  struct seq {
    unsigned long s;
    sequence<octet> xseq;
  };
};
